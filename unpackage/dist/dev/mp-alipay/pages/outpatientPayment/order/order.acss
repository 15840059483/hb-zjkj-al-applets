.no-list {
  margin-top: 1rem;
}
.no-list ._div {
  width: 2rem;
  height: 2rem;
  margin: 0 auto;
  border-radius: 50%;
  background-color: #b8b8b8;
  text-align: center;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.no-list ._div ._img {
  width: 60%;
  height: 60%;
}
.no-list ._p {
  text-align: center;
  margin-top: .2rem;
  color: #4d4d4d;
  font-size: .35rem;
  letter-spacing: 1px;
}
.switch-patient-bg {
  position: fixed;
  top: 0;
  width: 100%;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 2002;
}
.switch-patient-bg .switch-patient-container {
  position: absolute;
  bottom: 0;
  width: 100%;
  background-color: #fff;
}
.switch-patient-bg .switch-patient-container .border-bottom {
  border-bottom: 1px solid #e1e1e1;
}
.switch-patient-bg .switch-patient-container .switch-patient-title {
  padding: 16px 0;
  position: relative;
  font-weight: bold;
  text-align: center;
}
.switch-patient-bg .switch-patient-container .switch-patient-title text {
  position: absolute;
  right: 25px;
  top: 16px;
  color: #e3e7e6;
  font-size: 20px;
}
.switch-patient-bg .switch-patient-container .switch-patient-list {
  padding: 14px .4rem;
  position: relative;
}
.switch-patient-bg .switch-patient-container .switch-patient-list .patient-name {
  margin-bottom: .4rem;
  color: #080808;
  font-size: 14px;
  font-weight: bold;
}
.switch-patient-bg .switch-patient-container .switch-patient-list text {
  position: absolute;
  right: 40px;
  top: 30px;
  font-size: 24px;
}
.switch-patient-bg .switch-patient-container .switch-patient-btn {
  text-align: center;
  margin: .2rem 0;
  padding: .2rem 0;
  color: #008cfe;
}
.switch-patient-bg .switch-patient-container .switch-patient-btn-l {
  border-right: 1px solid #e1e1e1;
}
.xianzhi > .card-container {
  padding: .3rem .5rem;
  display: flex;
}
.xianzhi > .card-container .change-patient-name {
  text-align: right;
}


